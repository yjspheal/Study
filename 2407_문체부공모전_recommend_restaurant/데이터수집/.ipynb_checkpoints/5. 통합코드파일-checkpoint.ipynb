    "import pandas as pd\n",
    "import openai\n",
    "import pandas as pd\n",
    "\n",
    "final_df = pd.read_csv('./0714_final_df_with_summaries.csv')\n",
    "\n",
    "# OpenAI API 키 설정\n",
    "\n",
    "openai.api_key = API_KEY\n",
    "\n",
    "review = final_df.review_text_list\n",
    "\n",
    "# 리뷰 요약 함수\n",
    "def summarize_review(review):\n",
    "    \n",
    "    max_review_length = 10000  # 최대 리뷰 길이를 설정합니다\n",
    "    review = review[:max_review_length]  # 리뷰를 최대 길이로 잘라냅니다\n",
    "\n",
    "    # 프롬프트 생성\n",
    "    prompt = (\n",
    "        \"다음의 리뷰를 요약해줘:\\n\\n\"\n",
    "        f\"리뷰: {review}\\n\\n\"\n",
    "        \"1 긍정적인 부분 요약 200토큰 이내로 요약\\n\"\n",
    "        \"2. 부정적인 부분 요약 200토큰 이내로 요약:\\n\"\n",
    "        \"3. 주력 메뉴와 그 맛에 대한 사람들의 공통적인 평가 요약 (200토큰 이내)\\n\"\n",
    "    )\n",
    "\n",
    "    # OpenAI API 호출\n",
    "    response = openai.chat.completions.create(\n",
    "        model=\"gpt-3.5-turbo\",\n",
    "        messages=[\n",
    "            {\"role\": \"system\", \"content\": \"You are a helpful assistant who summarizes the restaurant's reviews.\"},\n",
    "            {\"role\": \"user\", \"content\": prompt}\n",
    "        ],\n",
    "        max_tokens=1000,\n",
    "        n=1,\n",
    "        stop=None,\n",
    "        temperature=0.1\n",
    "    )\n",
    "\n",
    "   # 응답 텍스트 추출\n",
    "    summary = response.choices[0].message.content.strip()\n",
    "    return summary\n",
    "\n",
    "# final_df['review_summarized'] = \"\"\n",
    "total_reviews = len(final_df)\n",
    "\n",
    "\n",
    "final_df = final_df.reset_index(drop=True)\n",
    "\n",
    "    \n",
    "# 각 리뷰에 대해 요약 수행\n",
    "for i in range(115):\n",
    "# for i in range(115,len(final_df)):\n",
    "\n",
    "    review = final_df.at[i, 'review_text_list']\n",
    "    summary = summarize_review(review)\n",
    "    final_df.at[i, 'review_summarized'] = summary\n",
    "    \n",
    "    print(f\"Processed {i+1}/{total_reviews} reviews\")\n",
    "    final_df.to_csv('0727_final_df_with_summaries.csv', index=False, encoding='utf-8-sig')"
   ]
