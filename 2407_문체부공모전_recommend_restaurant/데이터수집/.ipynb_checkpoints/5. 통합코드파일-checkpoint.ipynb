    "import os\n",
    "import pandas as pd\n",
    "from selenium import webdriver\n",
    "from datetime import datetime\n",
    "\n",
    "# 데이터프레임 로드\n",
    "dong = pd.read_csv('./연남동.csv', dtype={'ID': str})\n",
    "\n",
    "# Chrome WebDriver 초기화\n",
    "driver = webdriver.Chrome()\n",
    "\n",
