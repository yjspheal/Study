    "            if new_height == last_height:\n",
    "                scroll_attempts += 1\n",
    "                if scroll_attempts >= max_scroll_attempts:\n",
    "                    break\n",
    "            else:\n",
    "                scroll_attempts = 0  # 높이가 변경되면 시도 횟수 초기화\n",
